   "source": [
    "race_fields = [\"white_per\", \"asian_per\", \"black_per\", \"hispanic_per\"]\n",
    "\n",
    "correlations[race_fields].plot(kind = \"barh\")\n",
    "plt.axvline(x = 0.25, c = \"grey\")\n",
